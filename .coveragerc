[run]
source = bot
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    */site-packages/*
    bot/handlers/*
    bot/presentation/*
    bot/app.py
    bot/db/encrypted_store.py
    health_check.py
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[xml]
output = coverage.xml
